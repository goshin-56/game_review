<div class="col">
  <table class="table">
    <tr>
      <th><h1>edit</h1></th>
    </tr>
  </table>
  <%= form_with model: @review, url: public_review_path do |f| %>
    <div class="form-group">
      <h4>タイトル</h4>
      <%= f.text_field :title, class: "form-control" %>
    </div>
    <div>
      <h3>タグ</h3>
    </div>
    <div class="form-group">
      <h4>レビュー</h4>
      <%= f.text_area :body, class: "form-control" %>
    </div>
    <%= f.submit "更新" %>
  <% end %>
</div>
